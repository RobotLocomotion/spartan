package cpf_lcmtypes;

// For use with the Contact Particle Filter algorithm. Encodes the location
// of a single contact point estimate.

struct single_contact_filter_estimate_t
{
  int64_t utime;
  string body_name;
  float contact_normal[3];
  float contact_position[3];
  float contact_force[3];
  float contact_position_in_world[3];
  float contact_force_in_world[3];
  float contact_normal_in_world[3];
}
